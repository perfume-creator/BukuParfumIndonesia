{
    "version": 2,
    "framework": null,
    "buildCommand": "",
    "outputDirectory": "public",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/favicon\\.(ico|svg)",
            "dest": "/favicon.$1"
        },
        {
            "src": "/favicon-96x96\\.png",
            "dest": "/favicon-96x96.png"
        },
        {
            "src": "/apple-touch-icon\\.png",
            "dest": "/apple-touch-icon.png"
        },
        {
            "src": "/site\\.webmanifest",
            "dest": "/site.webmanifest"
        },
        {
            "src": "/robots\\.txt",
            "dest": "/robots.txt"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/vendor/(.*)",
            "dest": "/vendor/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/downloads/(.*)",
            "dest": "/downloads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}